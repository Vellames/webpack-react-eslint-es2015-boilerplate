{
  "parser": "babel-eslint",
  "extends": ["airbnb-base"],
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "id-length": 0,
    "indent": [1, 2],
    "no-console": 0,
    "no-unused-vars": [1, {"vars": "local", "args": "none"}],
    "strict": 0,
    "class-methods-use-this": 0,
    "react/jsx-uses-vars": [2]
  }
}